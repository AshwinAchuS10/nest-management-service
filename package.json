    "@nestjs/common": "8.0.0",
    "@nestjs/core": "8.0.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/microservices": "8.0.0",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/swagger": "^6.1.4",
