    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "_moduleAliases": {
    "modules": "build/src/modules",
    "application": "build/src/application",
    "infrastructure": "build/src/infrastructure",
    "domain": "build/src/domain",
    "common": "build/src/common",
    "controllers": "build/src/controllers",
    "configuration": "build/src/configuration",
    "libraries": "build/src/libraries",
    "interfaces": "build/src/interfaces",
    "interface": "build/src/interface",
    "middleware": "build/src/middleware",
    "constants": "build/src/constants",
    "src": "build/src"
  },
  "dependencies": {
    "@azure/keyvault-secrets": "^4.6.0",
